//const {{modelName}} = require('../models/{{modelName}}');

exports.init = () => {
	app.get('{{endpoint}}', (req, res) => {
		let data = {}
    data.status = 200
    data.message = 'Not get allow.'
    res.status(200).json(data)
	});

  app.post('{{endpoint}}', (req, res) => {
    /**
      * @param Integer id
      *
      * @return json
      *
      */
    let id = req.param('id')
    
    // return
    res.status(200).send(id)      
  });
}
