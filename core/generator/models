let data = [
  {
    "id": 1,
    "name": "Jose D. Roger",
    "desc": "Pirate King."
  },
  {
    "id": 2,
    "name": "Monkey D. Luffy",
    "desc": "Captain."
  },
];

exports.example = () => {
  return new Promise((resolve, reject) => {
    try {
      console.log(data)
      // @return data
      resolve(data)
    } catch (error) {
      reject(error)
    }
  });
}